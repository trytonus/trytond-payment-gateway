<form string="Enter Card Information">
    <label name="swipe_data"/>
    <field name="swipe_data" widget="password"/>
    <label name="card_present"/>
    <field name="card_present"/>
    <label name="owner"/>
    <field name="owner" xfill="1"/>
    <newline/>
    <label name="number"/>
    <field name="number" xfill="1"/>
    <group colspan="4" col="6" id="card_data">
        <label name="expiry_month"/>
        <field name="expiry_month"/>
        <label name="expiry_year"/>
        <field name="expiry_year"/>
        <label name="csc"/>
        <field name="csc"/>
    </group>
</form>

